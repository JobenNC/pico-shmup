-- quadratic bezier
function bezier(t, p1, p2, p3)
	-- p=(1-t)^2*p1 +
	-- 2*(1-t)*t*p2 + (t^2)*p3
	
	-- remember, you have to cover
	-- x & y

	-- return location on bezier
	-- curve for a given t val
	resp = {}
	resp.x = ((1-t)^2)*p1.x
	 + 2*(1-t)*t*p2.x + (t^2)*p3.x
	resp.y = ((1-t)^2)*p1.y
	 + 2*(1-t)*t*p2.y + (t^2)*p3.y
	return resp
end

function aabb_collision(a, b)
 return a.x < b.x + b.w and
 	a.x + a.w > b.x and
 	a.y < b.y + b.h and
 	a.y + a.h > b.y
end

