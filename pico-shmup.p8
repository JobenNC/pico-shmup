function draw_starfield()

 for _ in all(starfield_1)
 do
 	pset(_.x, _.y, 1)
 	_.y = (_.y + 1) % 128
 end
 
 for _ in all(starfield_2)
 do
 	pset(_.x, _.y, 2)
 	_.y = (_.y + 3) % 128
 end
 
 for _ in all(starfield_3)
 do
 	pset(_.x, _.y, 3)
 	_.y = (_.y + 6) % 128
 end
end
